<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cartmanbot.logic.CopyPastaDetector" table="copypasta">
	<id name="copy_id" column="id">
		<generator class="native"></generator>
	</id>
	<property name="text" column="copy_text" type="string" />
	<property name="percent" column="percent_match" type="int" />
	</class>
</hibernate-mapping>